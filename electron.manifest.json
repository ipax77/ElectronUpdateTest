{
  "executable": "ElectronUpdateTest",
  "singleInstance": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ipax77/electronupdatetest.git"
  },
  "build": {
    "win": {
      "publish": [ "github" ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "wwwroot/images/icon-512x512.png"
    },
    "appId": "com.github.ipax77.electronupdatetest",
    "productName": "ElectronUpdateTest",
    "copyright": "Copyright Â© 2019 Philipp Hetzner",
    "buildVersion": "0.0.1",
    "compression": "maximum",
    "directories": {
      "output": "../../bin/Desktop",
      "buildResources": "../../../build"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  },
  "user": "ipax77",
  "project": "ElectronUpdateTest",
  "author": "Philipp Hetzner",
  "license": "GPL3"
}